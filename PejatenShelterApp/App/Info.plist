<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>NSPhotoLibraryUsageDescription</key>
	<string>Untuk menggunakan foto yang ada di galeri, Pejaten Shelter butuh akses ke galeri foto</string>
	<key>NSCameraUsageDescription</key>
	<string>Untuk mengambil foto dengan kamera, Pejaten Shelter App butuh akses ke kamera</string>
	<key>AIRTABLE_BASE_KEY</key>
	<string>$(AIRTABLE_BASE_KEY)</string>
